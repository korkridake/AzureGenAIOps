{
  "llm": {
    "defaultModel": "gpt-4",
    "fallbackModel": "gpt-35-turbo",
    "embeddingModel": "text-embedding-3-large",
    "maxTokens": 4000,
    "temperature": 0.7,
    "topP": 0.9,
    "frequencyPenalty": 0.0,
    "presencePenalty": 0.0
  },
  "rag": {
    "chunkSize": 1000,
    "chunkOverlap": 200,
    "maxDocuments": 5,
    "scoreThreshold": 0.7,
    "hybridSearch": true,
    "semanticSearch": true
  },
  "safety": {
    "contentFilter": {
      "enabled": true,
      "threshold": "medium"
    },
    "piiDetection": {
      "enabled": true,
      "categories": ["Person", "Email", "Phone", "SSN", "CreditCard"]
    },
    "jailbreakDetection": {
      "enabled": true,
      "sensitivity": "medium"
    }
  },
  "monitoring": {
    "telemetry": {
      "enabled": true,
      "sampling": 1.0
    },
    "metrics": {
      "enabled": true,
      "interval": 60
    },
    "logging": {
      "level": "INFO",
      "structured": true
    }
  },
  "caching": {
    "enabled": true,
    "ttl": 3600,
    "maxSize": 1000
  },
  "rateLimiting": {
    "enabled": true,
    "requestsPerMinute": 100,
    "tokensPerMinute": 10000
  }
}